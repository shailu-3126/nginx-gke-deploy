apiVersion: apps/v1
kind: Deployment
metadata:
  name: backend3
spec:
  replicas: 1
  selector:
    matchLabels:
      app: backend3
  template:
    metadata:
      labels:
        app: backend3
    spec:
      containers:
      - name: backend3
        image: asia-south1-docker.pkg.dev/test-kumar-makala/fullstack-repo/backend3:v1
        ports:
        - containerPort: 8080
---
apiVersion: v1
kind: Service
metadata:
  name: backend3-svc
spec:
  selector:
    app: backend3
  ports:
  - protocol: TCP
    port: 8080
    targetPort: 8080
  type: ClusterIP
